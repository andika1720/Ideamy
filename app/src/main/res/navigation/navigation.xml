        tools:layout="@layout/fragment_change_password" />

    <fragment
        android:id="@+id/detailPaymentFragment"
        android:name="com.example.thefinalproject.ui.fragment.DetailPaymentFragment"
        android:label="fragment_detail_payment"
        tools:layout="@layout/fragment_detail_payment" >
        <action
            android:id="@+id/action_detailPaymentFragment_to_detailCourse"
            app:destination="@id/detailCourse" />
    </fragment>
    <fragment
        android:id="@+id/detailCourse"
        android:name="com.example.thefinalproject.ui.fragment.DetailCourse"
        android:label="fragment_detail_course"
