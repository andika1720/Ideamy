        <action
            android:id="@+id/action_homeFragment2_to_myClassFragment22"
            app:destination="@id/myClassFragment2" />
        <action
            android:id="@+id/action_homeFragment2_to_myCourseFragment2"
            app:destination="@id/myCourseFragment2" />
        <action
